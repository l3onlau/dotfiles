URxvt.font: xft:Noto Sans Mono:size=10:antialias=true, \
            xft:Noto Sans Mono CJK SC:size=10:antialias=true
!            xft:Noto Color Emoji:style=regular:size=9:antialias=false !not shown unless URxvt.lineSpace set to 100
URxvt.letterSpace: 0
URxvt.lineSpace: 0

URxvt.scrollTtyOutput:   False
URxvt.scrollWithBuffer:  True
URxvt.scrollTtyKeypress: True

URxvt.secondaryScroll: False

URxvt.keysym.Control-Insert:    eval:selection_to_clipboard
URxvt.keysym.Shift-Insert:      eval:paste_clipboard

URxvt.perl-ext-common:

URxvt.cursorBlink: true

URxvt.iso14755: false
URxvt.iso14755_52: false

